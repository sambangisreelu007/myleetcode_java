// 724. Find Pivot Index

class Solution {
    public int pivotIndex(int[] nums) {
     int n=nums.length;
     int[]LS=new int[n];
     int[]RS=new int[n];
     for(int i=1;i<n;i++){
        LS[i]=LS[i-1]+nums[i-1];
     }   
     for(int i=n-2;i>=0;i--){
        RS[i]=RS[i+1]+nums[i+1];
     }
     for(int i =0;i<n;i++){
        if(LS[i]==RS[i]){
            return i;
        }
           
        }
        return -1;
     }
    }
