// 3146. Permutation Difference between  Two Strings

class Solution {
    public int findPermutationDifference(String s, String t) {
        int diff=0;
        for(int i=0; i<s.length(); i++){
            char ch=s.charAt(i);
            int idx=t.indexOf(ch);
            diff+=Math.abs(i-idx);
        }
        return diff;
    }
}
