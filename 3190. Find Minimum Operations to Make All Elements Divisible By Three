// 3190. Find Minimum Operations to Make All Elements Divisible By Three

class Solution {
    public int minimumOperations(int[] nums) {
        int n=nums.length;
        int count=0;
        for(int i=0;i<n;i++){
            if(nums[i]%3==2){
                count+=1;
            }else if(nums[i]%3==1){
                count+=1;
            }else{
                count+=0;
              }
            }
            return count;
        }
  }


